cflags = -DTORCH_EXTENSION_NAME=upfirdn2d -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include/TH -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include/THC -isystem /home/jcbgb/anaconda3/envs/maua/include -isystem /home/jcbgb/anaconda3/envs/maua/include/python3.9 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++14
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=upfirdn2d -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include/TH -isystem /home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/include/THC -isystem /home/jcbgb/anaconda3/envs/maua/include -isystem /home/jcbgb/anaconda3/envs/maua/include/python3.9 -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_61,code=sm_61 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 --compiler-options '-fPIC' -std=c++14
cuda_post_cflags = 
ldflags = -shared -L/home/jcbgb/anaconda3/envs/maua/lib/python3.9/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda_cu -ltorch_cuda_cpp -ltorch -ltorch_python -L/home/jcbgb/anaconda3/envs/maua/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags

rule link
  command = $cxx $in $ldflags -o $out

